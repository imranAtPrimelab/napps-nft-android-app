<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_history_view_pager"
    app:startDestination="@id/allHistoryFragment">

    <fragment
        android:id="@+id/allHistoryFragment"
        android:name="com.nearlabs.nftmarketplace.ui.main.historySubFragments.AllHistoryFragment"
        android:label="AllHistoryFragment" />

    <fragment
        android:id="@+id/recievedHistoryFragment"
        android:name="com.nearlabs.nftmarketplace.ui.main.historySubFragments.RecievedHistoryFragment"
        android:label="RecievedHistoryFragment" />
    <fragment
        android:id="@+id/sentHistoryFragment"
        android:name="com.nearlabs.nftmarketplace.ui.main.historySubFragments.SentHistoryFragment"
        android:label="SentHistoryFragment" />

    <action
        android:id="@+id/toAllHistory"
        app:destination="@+id/allHistoryFragment" />
    <action
        android:id="@+id/toRecievedHistory"
        app:destination="@+id/recievedHistoryFragment" />
    <action
        android:id="@+id/toSentHistory"
        app:destination="@+id/sentHistoryFragment" />

</navigation>